#include <stdio.h>
#include <math.h>
int main(int argc, char * argv[] ){

float a, b, c, x1, x2;
float delta;
printf("coloque a");
scanf ("%f", &a);

printf("coloque b");
scanf ("%f", &b);

printf("coloque c");

scanf ("%f", &c);

delta=(b*b) - 4*a*c;

if(delta==0){
    x1=x2= (-b/(2*a));
printf("raiz igual a %.2f", x1);
}

if(delta>0){
    x1=((-b + sqrt(delta))/2)/10;
    x2=((-b - sqrt(delta))/2)/10;
        printf("x1 igual a %.2f", x1);
        printf(" x2 igual a %.2f", x2);
}
if(delta<0){
        printf("raiz n real");}

return 0;
}
